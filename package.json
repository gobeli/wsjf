{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon -L",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "frontend:watch": "cross-env NODE_ENV=development webpack --watch --watch-poll=1000",
    "css:build": "node-sass --include-path scss frontend/style/main.scss backend/assets/css/main.css",
    "css:watch": "nodemon -L --config nodemon-styles.json",
    "dev": "concurrently --kill-others \"npm run start:watch\" \"npm run frontend:watch\" \"npm run css:watch\""
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.5.8",
    "buefy": "^0.6.3",
    "connect-typeorm": "^1.0.2",
    "dotenv": "^5.0.0",
    "express-session": "^1.15.6",
    "https-proxy-agent": "^2.1.1",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "typeorm": "^0.1.12",
    "typescript": "^2.6.2",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.3",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "file-loader": "^1.1.9",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.1",
    "sass-loader": "^6.0.6",
    "ts-node": "^4.1.0",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0"
  }
}
